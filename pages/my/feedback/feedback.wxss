.f-box {
  height: 100%;
  display: flex;
  padding: 32px 16px 0 16px;
  flex-direction: column;
  background-color: white;
}
.label {
  color: #333333;
  font-size: 32rpx;
  margin-top: 48rpx;
  margin-bottom: 16rpx;
}
.input{
  width: 96%;
  font-size: 30rpx;
  padding: 20rpx 2%;
  border-radius: 8rpx;
  border-bottom: solid 1px #dddddd;
}
.inputs {
  width: 96%;
  height: 180px;
  font-size: 30rpx;
  padding: 2%;
  border-radius: 16rpx;
  border: solid 1px #e3e3e3;
}
.btn {
  width: 100%;
  height: 88rpx;
  display: flex;
  margin-top: 32px;
  color: white;
  font-size: 36rpx;
  font-weight: 700;
  align-items: center;
  justify-content: center;
  border-radius: 8px;
  background-color: #3c9e04;
}