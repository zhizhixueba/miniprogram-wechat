/*  */
.title {
  color: #232323;
  font-size: 34rpx;
  line-height: 56rpx;
  font-weight: 500;
  font-family: PingFangSC-Medium;
  /* font-family: PingFangSC-Regular; */
}
.subtitle {
  color: #393939;
  font-size: 30rpx;
  line-height: 40rpx;
}

.value {
  color: #323232;
  font-size: 28rpx;
}
.label {
  color: #323232;
  font-size: 28rpx;
}
.desc {
  color: #636363;
  font-size: 28rpx;
  line-height: 34rpx;
}
.info {
  color: #636363;
  font-size: 28rpx;
}
.date {
  color: #979797;
  font-size: 24rpx;
}

.ellips {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.ellip {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
}
.indent {
  text-indent: 2em;
}
